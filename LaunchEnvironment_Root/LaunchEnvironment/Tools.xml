<?xml version="1.0" encoding="utf-8" ?>
<RuntimeInfo>
  <!--To run always as admin then enable this value-->
  <!--<RunasAdmin>true</RunasAdmin>-->
  <ToolPath>
    <Tool>
      <Name>Generic</Name>
      <!--<Path>%SystemRoot%\system32\cmd.exe</Path>-->
    </Tool>
  </ToolPath>
  <ContextMenu>
    <Menu>VSCode</Menu>
    <Menu>VSStudio</Menu>
    <Menu>PowerShell</Menu>
    <Menu>Cmd</Menu>
  </ContextMenu>
  <ToolBar>
    <ToolBarItem>
      <Name>VSCode</Name>
      <Tool>
        <Name>VSCode</Name>
        <Path>%ToolDir%\VSCode\Code.exe</Path>
        <BaseDir>%ToolDir%\VSCode</BaseDir>
      </Tool>
    </ToolBarItem>
    <ToolBarItem>
      <Name>VSStudio</Name>
      <Tool>
        <Name>VSStudio</Name>
        <Path>%VSStudioDir%\Common7\IDE\devenv.exe</Path>
        <BaseDir>%VSStudioDir%</BaseDir>
      </Tool>
    </ToolBarItem>
    <ToolBarItem>
      <Name>Run</Name>
      <Tool>
        <Name>Run</Name>
        <Path>%LaunchEnvExeDir%\runscript.exe</Path>
        <Arguments>-rundiag</Arguments>
      </Tool>
    </ToolBarItem>
    <ToolBarItem>
      <Name>PowerShell</Name>
      <Tool>
        <Name>PowerShell</Name>
        <Path>%SystemRoot%\system32\WindowsPowerShell\v1.0\powershell.exe</Path>
      </Tool>
      <Tool>
        <Name>PowerShell-32bit</Name>
        <Path>%SystemRoot%\syswow64\WindowsPowerShell\v1.0\powershell.exe</Path>
      </Tool>
    </ToolBarItem>
    <ToolBarItem>
      <Name>Cmd</Name>
      <Tool>
        <Name>Cmd</Name>
        <Path>%SystemRoot%\system32\cmd.exe</Path>
        <Path32Bit>%SystemRoot%\syswow64\cmd.exe</Path32Bit>
      </Tool>
      <Tool>
        <Name>Cmd-32Bit</Name>
        <Path>%SystemRoot%\syswow64\cmd.exe</Path>
      </Tool>
    </ToolBarItem>
    <ToolBarItem>
      <Name>ConEmu</Name>
      <Tool>
        <Name>ConEmu</Name>
        <Path>%ToolDir%\ConEmu\ConEmu64.exe</Path>
        <BaseDir>%ToolDir%\ConEmu</BaseDir>
      </Tool>
      <Tool>
        <Name>ConEmu-32Bit</Name>
        <Path>%ToolDir%\ConEmu\ConEmu.exe</Path>
        <BaseDir>%ToolDir%\ConEmu</BaseDir>
      </Tool>
    </ToolBarItem>
    <ToolBarItem>
      <Name>Bash</Name>
      <Tool>
        <Name>Bash</Name>
        <Path>%SystemRoot%\system32\bash.exe</Path>
      </Tool>
    </ToolBarItem>
    <ToolBarItem>
      <Name>WinDbg</Name>
      <Tool>
        <Name>WinDbg</Name>
        <IsStoreApp>true</IsStoreApp>
        <Path>WinDbgX.exe</Path>
        <!--<Path>%SystemRoot%\system32\cmd.exe</Path>-->
      </Tool>
      <Tool>
        <Name>WinDbg-32Bit</Name>
        <IsStoreApp>true</IsStoreApp>
        <Path>WinDbgX.exe</Path>
        <!--<Path>%SystemRoot%\system32\cmd.exe</Path>-->
      </Tool>
    </ToolBarItem>
    <ToolBarItem>
      <Name>Python</Name>
      <Tool>
        <Name>Python</Name>
        <!--<Path>%SystemRoot%\system32\cmd.exe</Path>-->
      </Tool>
      <Tool>
        <Name>WingIDE</Name>
        <Path>%ToolDir%\wingide\bin\wing-personal.exe</Path>
        <BaseDir>%ToolDir%\wingide</BaseDir>
      </Tool>
      <Tool>
        <Name>Komodo</Name>
        <Path>%ToolDir%\komodoportable\tool\KomodoIDEPortable.exe</Path>
        <BaseDir>%ToolDir%\komodoportable</BaseDir>
      </Tool>
    </ToolBarItem>
  </ToolBar>
  <MenuBar>
    <MenuBar>
      <Name>Integration</Name>
      <Tools>
        <Tool>
          <Name>Register Explorer ContextMenu</Name>
          <Editor>RegisterExplorerContextMenu</Editor>
        </Tool>
        <Tool>
          <Name>Write Config Registry Values</Name>
          <Editor>WriteConfigRegistryValues</Editor>
        </Tool>
        <Tool>
          <Name>Update Python Script Folder</Name>
          <Editor>UpdatePythonScriptFolder</Editor>
        </Tool>
      </Tools>
    </MenuBar>
    <MenuBar>
      <Name>Editors</Name>
      <Tools>
        <Tool>
          <Name>Arduino</Name>
          <Path>%ToolDir%\Arduino\tool\Arduino.exe</Path>
          <BaseDir>%ToolDir%\Arduino</BaseDir>
        </Tool>
        <Tool>
          <Name>SharpDevelop</Name>
          <Path>%ToolDir%\sharpdevelop\bin\SharpDevelop.exe</Path>
          <BaseDir>%ToolDir%\sharpdevelop</BaseDir>
        </Tool>
        <Tool>
          <Name>CodeBlocks</Name>
          <Path>%ToolDir%\codeblocks\tool\codeblocks.exe</Path>
          <BaseDir>%ToolDir%\codeblocks</BaseDir>
          <Arguments>/na /nd</Arguments>
        </Tool>
        <Tool>
          <Name>CodeLite</Name>
          <Path>%ToolDir%\codelite\tool\codelite.exe</Path>
          <BaseDir>%ToolDir%\codelite</BaseDir>
          <!--<Envs>
        <EnviromentVariable Action="Overwrite">
          <Name>LOCALAPPDATA</Name>
          <Value>%ToolDir%\codelite\userdata</Value>
        </EnviromentVariable>
      </Envs>-->
        </Tool>
      </Tools>
    </MenuBar>
    <MenuBar>
      <Name>Tools</Name>
      <Tools>
        <Tool>
          <Name>MobXTerm</Name>
          <Path>%ProgramFiles(x86)%\Mobatek\MobaXterm\MobaXterm.exe</Path>
          <BaseDir>%ProgramFiles(x86)%\Mobatek</BaseDir>
        </Tool>
        <Tool>
          <Name>Notepad</Name>
          <Path>%systemroot%\notepad.exe</Path>
          <!--<ByPassRegistry>true</ByPassRegistry>-->
          <BaseDir>%systemroot%</BaseDir>
        </Tool>
        <Tool>
          <Name>Testing</Name>
          <Path>%LaunchEnvExeDir%\Testing.exe</Path>
          <!--<ByPassRegistry>true</ByPassRegistry>-->
          <BaseDir>%LaunchEnvExeDir%</BaseDir>
          <Envs>
            <EnviromentVariable Action="Overwrite">
              <Name>LOCALAPPDATA</Name>
              <Value>%ToolDir%</Value>
            </EnviromentVariable>
          </Envs>
        </Tool>
      </Tools>
    </MenuBar>
  </MenuBar>
  <Envs>
    <EnviromentVariable>
      <Name>PATH</Name>
      <Value>.</Value>
    </EnviromentVariable>
    <EnviromentVariable>
      <Name>ToolDir</Name>
      <Value>E:\devtools</Value>
    </EnviromentVariable>
    <EnviromentVariable>
      <Name>SysGCC</Name>
      <Value>C:\SysGCC</Value>
    </EnviromentVariable>
    <EnviromentVariable>
      <Name>MyTools</Name>
      <Value>K:</Value>
    </EnviromentVariable>
  </Envs>
</RuntimeInfo>