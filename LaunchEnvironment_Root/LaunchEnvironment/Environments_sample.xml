      <InstallPath>%ToolDir%\activepython35</InstallPath>
      <RegConfigs>
        <RegKey>
          <Key>HKEY_LOCAL_MACHINE\SOFTWARE\Python\ActivePython35\3.5</Key>
          <RegValues>
            <RegValue Type="String" >
