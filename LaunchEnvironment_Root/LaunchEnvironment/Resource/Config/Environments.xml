    <!--WinDbg-->
    <Config>
      <Name>Windbg</Name>
      <Id>WinDbg</Id>
      <ConfigPath>%WinDbgDir%\Debuggers\Windbg</ConfigPath>
      <Envs>
        <EnviromentVariable>
          <Name>PATH</Name>
          <Value>%WinDbgDir%\Debuggers\Scripts</Value>
        </EnviromentVariable>
        <EnviromentVariable>
          <Name>Scripts</Name>
          <Value>%WinDbgDir%\Debuggers\Scripts</Value>
        </EnviromentVariable>
        <EnviromentVariable>
          <Name>_NT_SYMBOL_PATH</Name>
          <Value>srv*C:\DebugSymbols\Microsoft*https://msdl.microsoft.com/download/symbols</Value>
        </EnviromentVariable>
      </Envs>
    </Config>

