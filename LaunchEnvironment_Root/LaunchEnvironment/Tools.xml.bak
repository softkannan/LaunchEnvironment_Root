  </MenuBar>
  <!--Apply to all environment variables-->
  <Envs>
    <EnviromentVariable>
      <Name>PATH</Name>
      <Value>.</Value>
    </EnviromentVariable>
    <EnviromentVariable>
      <Name>ToolDir</Name>
      <Value>E:\devtools</Value>
    </EnviromentVariable>
    <EnviromentVariable>
      <Name>SysGCC</Name>
      <Value>C:\SysGCC</Value>
    </EnviromentVariable>
    <EnviromentVariable>
      <Name>MyTools</Name>
      <Value>K:</Value>
    </EnviromentVariable>
      <EnviromentVariable>
      <Name>WinDbgDir</Name>
      <Value>..\Debuggers_Win7</Value>
    </EnviromentVariable>
  </Envs>
</RuntimeInfo>